generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "postgresql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model LikedProperty {
  id            String   @id @default(uuid())
  zpid          String
  userId        String
  createdAt     DateTime @default(now())
  address       String?
  price         String?
  listingStatus String?
  livingArea    String?
  propertyType  String?
}
